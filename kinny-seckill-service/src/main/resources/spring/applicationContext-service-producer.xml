<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:dubbo="http://code.alibabatech.com/schema/dubbo"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd http://code.alibabatech.com/schema/dubbo http://code.alibabatech.com/schema/dubbo/dubbo.xsd">


    <dubbo:application name="kinny-seckill-service"></dubbo:application>

    <dubbo:protocol port="20889"></dubbo:protocol>

    <dubbo:registry address="zookeeper://192.168.25.130:2181"></dubbo:registry>

    <dubbo:annotation package="com.kinny.seckill.service.impl"></dubbo:annotation>


    <bean id="idWorker"
          class="com.kinny.util.IdWorker">
        <constructor-arg name="datacenterId" value="0"></constructor-arg>
        <constructor-arg name="workerId" value="0"></constructor-arg>
    </bean>

</beans>